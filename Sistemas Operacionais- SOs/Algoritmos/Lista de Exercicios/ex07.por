programa {

  real numero, dobro, terca_parte
  funcao inicio() {
    //entrada

    escreva("Digite um número real : " )
    leia(numero)

//processamento

dobro = numero * 2
terca_parte = numero / 3

//saida

escreva( " O dobro de " , numero , " é " , dobro , "\n")
escreva(" A terça_parte de " , numero ," é " , terca_parte )
  }
}
