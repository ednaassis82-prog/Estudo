programa {

   real Salario, Aumento
   

  funcao inicio() {
     //saida

      escreva ("Digite o salário do funcionário: ")
      leia (Salario)

   

     //processamento

      Aumento= Salario*0.15
      Salario= Salario+Aumento

     //Saída
      escreva ("Seu novo salário é ", Salario)
 
  }
}
