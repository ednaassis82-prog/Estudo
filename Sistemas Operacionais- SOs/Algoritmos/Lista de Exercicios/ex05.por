programa {
real nota1, nota2, media

  funcao inicio() {
  //entrada

  escreva( " Digite a  primeira nota : " ) 
  leia( nota1 ) 
  escreva( " Digite a segunda nota :  " )
  leia( nota2 )

  //processamento

  media = (nota1+nota2/2)


  //saida

  escreva( " Digite a média entre nota1 e nota2 "," é igual a " , media )
  }
}
